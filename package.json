{
  "name": "etex-typescript-migration",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "bin": {
    "tex": "bin/tex",
    "initex": "bin/initex"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:probe": "fpc -v0 -O- -o./scripts/probe_math ./scripts/probe_math.pas",
    "test": "npm run build:probe && npm run build && node --test test/*.test.js",
    "trip": "bash ./scripts/run_trip.sh --suite both",
    "trip:trip": "bash ./scripts/run_trip.sh --suite trip",
    "trip:etrip": "bash ./scripts/run_trip.sh --suite etrip"
  }
}
